    "type": "contao-module",
    "authors" : [{
        "name" : "chuckki",
        "email" : "https://github.com/chuckki"
    }],
    "license": "LGPL-3.0+",
    "require": {
        "php": "~5.4 || ~7.0",
        "contao/core-bundle":"~3.5 || ~4.1",
